{
	"name": "crystalize.js",
	"version": "0.0.5",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"/dist"
	],
	"scripts": {
		"test": "jest",
		"test:coverage": "npm test -- --coverage",
		"test:badges": "npm run test:coverage  && jest-coverage-badges",
		"build": "tsc",
		"prepare": "husky install"
	},
	"author": "zachbutton",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/zachbutton/crystalize.js.git"
	},
	"bugs": {
		"url": "https://github.com/zachbutton/crystalize.js/issues"
	},
	"homepage": "https://github.com/zachbutton/crystalize.js#readme",
	"description": "",
	"devDependencies": {
		"@types/jest": "^29.5.4",
		"@types/seamless-immutable": "^7.1.16",
		"husky": "^8.0.3",
		"jest": "^29.6.4",
		"jest-coverage-badges": "^1.1.2",
		"lint-staged": "^14.0.1",
		"prettier": "^3.0.3",
		"seamless-immutable": "^7.1.4",
		"ts-jest": "^29.1.1",
		"typescript": "^5.2.2"
	},
	"lint-staged": {
		"*.{js,css,md}": "prettier --write"
	},
	"jest": {
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.tsx?$",
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"coverageReporters": [
			"json-summary",
			"text",
			"lcov"
		]
	}
}
